<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.constraint_layout">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="constraint_layout"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.constraint_layout.MainActivity"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.constraint_layout.ConstraintActivity"
            android:configChanges="orientation"/>
        <activity
            android:name="com.example.constraint_layout.RelativeActivity"
            android:configChanges="orientation"/>
        <activity
            android:name="com.example.constraint_layout.NoActivity"
            android:configChanges="orientation"/>
    </application>

</manifest>